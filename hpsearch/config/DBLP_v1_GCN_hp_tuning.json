{
    "dataset": {
        "class": "src.dataset.dataset_base.Dataset",
        "parameters": {
            "generator": {
                "class": "src.dataset.generators.dblp_v1.DBLP",
                "parameters": {"data_dir": "data/datasets/DBLP_v1/", "dataset_name": "DBLP_v1"}
            }
        }
    },
    "oracle": {
        "class": "hpsearch.oracle.torch.OracleTorch",
        "parameters": {
            "epochs": 500,
            "batch_size": 64,
            "optimizer": {
                "class": "torch.optim.AdamW",
                "parameters": {
                    "lr":0.005597827355896938,
                    "weight_decay":0.022451620056299467
                }
            },
            "loss_fn": {
                "class": "torch.nn.CrossEntropyLoss",
                "parameters": {
                    "reduction":"mean"
                }
            },
            "model": {
                "class": "src.oracle.nn.gcn.DownstreamGCN",
                "parameters": {
                    "num_conv_layers":5,
                    "num_dense_layers":9,
                    "conv_booster":2,
                    "linear_decay":0.3211868888719398
                }
            }
        }
    }
}