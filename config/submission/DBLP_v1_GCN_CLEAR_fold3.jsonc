    "explainers": [{"class": "src.explainer.generative.clear.CLEARExplainer","parameters":{
        "epochs": 50,
        "lambda_cfe": 0.5,
        "lambda_kl": 1.0,
        "lambda_sim": 1.0,
        "alpha": 4,
        "dropout": 0.2,
        "batch_size_ratio": 0.8,
        "lr": 0.05
    }}],
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json"
